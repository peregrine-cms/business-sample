<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:per="http://www.peregrine-cms.com/jcr/cms/1.0"
    jcr:description="pagelist themecleanflex component library"
    jcr:primaryType="per:Page"
    jcr:title="tf-cl | pagelist">
    <jcr:content
        jcr:primaryType="per:PageContent"
        jcr:title="pagelist"
        sling:resourceType="business_sample/components/page"
        brand="themecleanflex"
        description="pagelist themecleanflex component library"
        ogImage=""
        ogImageAlt=""
        ogTitle=""
        ogType="website"
        ogUrl=""
        template="/content/business_sample/templates"
        useDescription="{Boolean}true"
        useTitle="{Boolean}true">
        <clheader
            jcr:primaryType="nt:unstructured"
            sling:resourceType="business_sample/components/header"
            backgroundtype="color"
            bgcolor="#eeeeee"
            bgvideo="https://www.youtube.com/embed/Ju86mknumYM"
            bottompadding="10"
            buttonsize="default"
            collapsed="false"
            colorscheme="light"
            htmlelement="header"
            logo="/content/business_sample/assets/samples/peregrine-logo.svg"
            logoalttext="Peregrine CMS Logo"
            logosize="60"
            logourl="/content/business_sample/pages/library.html"
            toppadding="10">
            <elevation jcr:primaryType="nt:unstructured"/>
            <links jcr:primaryType="nt:unstructured"/>
            <buttons jcr:primaryType="nt:unstructured"/>
        </clheader>
        <clbreadcrumb
            jcr:primaryType="nt:unstructured"
            sling:resourceType="business_sample/components/breadcrumb"
            backgroundtype="color"
            bgcolor="#eeeeee"
            colorscheme="light"
            custombackground="false"
            htmlelement="nav"
            level="2">
            <toppadding jcr:primaryType="nt:unstructured"/>
            <bottompadding jcr:primaryType="nt:unstructured"/>
        </clbreadcrumb>
        <pager0
            jcr:primaryType="nt:unstructured"
            sling:resourceType="business_sample/components/pager"
            backgroundtype="color"
            bgcolor="#eeeeee"
            bottompadding="30"
            colorscheme="light"
            custombackground="false"
            htmlelement="nav"
            nextlabel="next >"
            prevlabel="&lt; previous"
            toppadding="30"/>
        <container8
            jcr:primaryType="nt:unstructured"
            sling:resourceType="business_sample/components/container"
            htmlelement="main">
            <intro1
                jcr:primaryType="nt:unstructured"
                sling:resourceType="business_sample/components/richtext"
                backgroundtype="color"
                bgcolor="#eeeeee"
                bottompadding="25"
                custombackground="true"
                htmlelement="banner"
                text="&lt;h1 id=&quot;pagelist&quot;>Pagelist&lt;/h1> &lt;p>The Page List component displays the hierarchy of the current site to a given depth.&lt;/p> "
                toppadding="25"/>
            <title2
                jcr:primaryType="nt:unstructured"
                sling:resourceType="business_sample/components/richtext"
                backgroundtype="color"
                bgcolor="#eeeeee"
                bottompadding="25"
                custombackground="true"
                text="&lt;h2>Page List (Features)&lt;/h2>"
                toppadding="25"/>
            <pagelist3
                jcr:primaryType="nt:unstructured"
                sling:resourceType="business_sample/components/pagelist"/>
            <title4
                jcr:primaryType="nt:unstructured"
                sling:resourceType="business_sample/components/richtext"
                backgroundtype="color"
                bgcolor="#eeeeee"
                bottompadding="25"
                custombackground="true"
                text="&lt;h2>Page List - Example (Features)&lt;/h2>"
                toppadding="25"/>
            <pagelist5
                jcr:primaryType="nt:unstructured"
                sling:resourceType="business_sample/components/pagelist"
                bottompadding="20"
                levels="2"
                rootpage="/content/business_sample/pages"
                toppadding="20"/>
            <title6
                jcr:primaryType="nt:unstructured"
                sling:resourceType="business_sample/components/richtext"
                backgroundtype="color"
                bgcolor="#eeeeee"
                bottompadding="25"
                custombackground="true"
                text="&lt;h2>Page List no default values (Empty)&lt;/h2>"
                toppadding="25"/>
            <pagelist7
                jcr:primaryType="nt:unstructured"
                sling:resourceType="business_sample/components/pagelist"/>
        </container8>
        <pager9
            jcr:primaryType="nt:unstructured"
            sling:resourceType="business_sample/components/pager"
            backgroundtype="color"
            bgcolor="#eeeeee"
            bottompadding="30"
            colorscheme="light"
            custombackground="false"
            htmlelement="nav"
            nextlabel="next >"
            prevlabel="&lt; previous"
            toppadding="30"/>
        <clfooter
            jcr:primaryType="nt:unstructured"
            sling:resourceType="business_sample/components/footer"
            backgroundtype="color"
            bgcolor="#eeeeee"
            bottompadding="0"
            colorscheme="light"
            copyright="Copyright 2019 Peregrine - All Rights Reserved"
            htmlelement="footer"
            logo="/content/business_sample/assets/samples/peregrine-logo.svg"
            logoalttext="Peregrine CMS Logo"
            logosize="60"
            logourl="/content/business_sample/pages/library.html"
            showlogo="true"
            toppadding="30">
            <columns jcr:primaryType="nt:unstructured">
                <c3
                    jcr:primaryType="nt:unstructured"
                    text=""
                    title=""/>
                <c4
                    jcr:primaryType="nt:unstructured"
                    text="&lt;p>info@headwire.com&lt;br>&lt;/p>"
                    title="Contact"/>
                <c5
                    jcr:primaryType="nt:unstructured"
                    text="&lt;p>23293 S Pointe Drive, Laguna Hills, CA 92653&lt;/p>"
                    title="Location"/>
            </columns>
            <icons jcr:primaryType="nt:unstructured"/>
        </clfooter>
    </jcr:content>
    <sample/>
    <sample-blank/>
</jcr:root>
