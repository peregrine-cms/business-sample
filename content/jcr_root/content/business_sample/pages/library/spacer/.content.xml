                jcr:primaryType="nt:unstructured"
                sling:resourceType="business_sample/components/spacer"
                backgroundtype="image"
                bgimage="/content/business_sample/assets/samples/beach-bird-s-eye-view-daytime-1249586.jpg"
                custombackground="true"
                height="80"/>
            <title6
                jcr:primaryType="nt:unstructured"
                sling:resourceType="business_sample/components/richtext"
                backgroundtype="color"
                bgcolor="#eeeeee"
                bottompadding="25"
